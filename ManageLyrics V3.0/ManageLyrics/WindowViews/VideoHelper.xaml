<Window x:Class="ManageLyrics.VideoHelper"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        mc:Ignorable="d" Visibility="Visible"
        Title="VideoHelper" Height="490" Width="800" Loaded="Window_Loaded">
    <Grid>
        <StackPanel>
            <MediaElement x:Name="mediaElement" LoadedBehavior="Manual" Visibility="Visible" Stretch="Fill" />
        </StackPanel>
    </Grid>
</Window>
